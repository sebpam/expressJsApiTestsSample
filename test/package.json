  "bugs": {
    "url": "https://github.com/sebpam/expressJsApiTestsSample/issues"
  },
  "homepage": "https://github.com/sebpam/expressJsApiTestsSample#readme",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "chai": "^4.3.7",
    "jsonschema": "^1.4.1",
    "mocha": "^10.2.0",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "moment": "^2.29.4",
    "mysql2": "^3.2.0",
    "mysql2-promise": "^0.1.4",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1"
  }
